														"example": "9000000000",
														"length": 10
													},
													"birth_date": {
														"type": "string",
														"example": "dd/mm/yy",
													},
													"gender": {
														"type": "string",
														"example": "M",
														"length": 1,
													}
												}
											}
										}
									}
								}
							}
						}
					}
				}
